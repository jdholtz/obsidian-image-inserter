		"obsidian-image"
	],
	"author": "Joey Holtzman",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^20.10.7",
		"@typescript-eslint/eslint-plugin": "^5.29.0",
		"@typescript-eslint/parser": "^5.29.0",
		"builtin-modules": "^3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "^1.4.11",
		"tslib": "^2.4.0",
		"typescript": "^4.7.4"
	}
}
